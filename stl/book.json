{
  "title": "STL 语言参考手册",
  "description": "本书主要针对那些初次接触SiteServer CMS产品的用户，借助此文档可以快速了解SiteServer CMS 产品的一些基本概念和快速上手一些常用的基本操作。",
  "author": "SiteServer CMS",
  "output.name": "site",
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "root": ".",
  "structure": {
    "readme": "readme.md"
  },
  "links": {
    "sidebar": {
      "STL 语言参考手册": "index.html"
    }
  },
  "plugins": [
    "-lunr",
    "-search",
    "search-plus",
    "ga",
    "-sharing",
    "sharing-plus",
    "favicon",
    "splitter",
    "tbfed-pagefooter",
    "multipart",
    "collapsible-menu-ex"
  ],
  "pluginsConfig": {
    "sharing": {
      "douban": true,
      "linkedin": true,
      "qq": true,
      "qzone": true,
      "weibo": true,
      "all": [
        "douban",
        "weibo",
        "linkedin",
        "qq",
        "qzone"
      ]
    },
    "ga": {
      "token": "UA-1398374-1"
    },
    "favicon": {
      "shortcut": "assets/favicon.ico",
      "bookmark": "assets/favicon.ico",
      "appleTouch": "assets/favicon.ico",
      "appleTouchMore": {
        "120x120": "assets/favicon.ico",
        "180x180": "assets/favicon.ico"
      }
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright &copy <a href='http://www.siteserver.cn'>北京百容千域软件技术开发有限责任公司</a> 2018",
      "modify_label": "该文件修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "theme-default": {
      "showLevel": false
    }
  }
}